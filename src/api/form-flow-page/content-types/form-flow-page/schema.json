    "stepId": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "backButtonLabel": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
