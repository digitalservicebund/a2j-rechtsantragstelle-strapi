      },
      "component": "page.meta-page-info-flow",
      "repeatable": false,
      "required": true
    },
    "preHeading": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "heading": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "nextButtonLabel": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "pre_form": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "basic.heading",
        "basic.paragraph",
        "page.box-with-image",
        "page.box",
        "page.info-box",
        "page.table-of-contents",
        "page.array-summary",
        "page.list",
        "page.inline-notice",
        "page.summary-overview-section",
        "page.email-capture"
      ]
    },
    "form": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "form-elements.input",
        "form-elements.textarea",
        "form-elements.select",
        "form-elements.checkbox",
        "form-elements.dropdown",
        "form-elements.tile-group",
        "form-elements.date-input",
        "form-elements.time-input",
        "form-elements.files-upload",
        "form-elements.auto-suggest-input",
        "form-elements.hidden-input",
        "form-elements.fieldset"
      ]
    },
    "post_form": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "basic.heading",
        "basic.paragraph",
        "page.box-with-image",
        "page.box",
        "page.info-box",
        "page.inline-notice",
        "page.table-of-contents",
        "page.list",
        "page.user-feedback",
        "page.email-capture"
      ]
    },
    "flow_ids": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::flow-id.flow-id"
