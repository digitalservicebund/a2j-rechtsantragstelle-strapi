{
  "kind": "collectionType",
  "collectionName": "result_pages",
  "info": {
    "singularName": "result-page",
    "pluralName": "result-pages",
    "displayName": "ResultPage",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "pageTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "heading": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "basic.heading",
      "repeatable": false,
      "required": true
    },
    "hintText": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "basic.paragraph",
      "repeatable": false,
      "required": false
    },
    "freeZone": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "basic.heading",
        "basic.link",
        "basic.paragraph",
        "page.info-box",
        "page.box-with-image",
        "page.list",
        "page.inline-notice",
        "page.details-summary",
        "page.box",
        "page.user-feedback"
      ]
    },
    "pageType": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "error",
      "enum": [
        "error",
        "success",
        "warning",
        "info"
      ]
    },
    "nextLink": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "basic.link",
      "repeatable": false
    },
    "documents": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::element-with-id.element-with-id"
    },
    "nextSteps": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::element-with-id.element-with-id"
    },
    "flow_ids": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::flow-id.flow-id"
    },
    "stepId": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "unique": false
    },
    "backButtonLabel": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "hintButton": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "form-elements.button",
      "repeatable": false
    }
  }
}
