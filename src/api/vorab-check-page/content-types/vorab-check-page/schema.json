{
  "kind": "collectionType",
  "collectionName": "vorab_check_pages",
  "info": {
    "singularName": "vorab-check-page",
    "pluralName": "vorab-check-pages",
    "displayName": "VorabCheckPage",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "pageTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "nextButtonLabel": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "pre_form": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "basic.heading",
        "basic.paragraph",
        "page.inline-notice",
        "page.details-summary"
      ]
    },
    "form": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "form-elements.input",
        "form-elements.textarea",
        "form-elements.select",
        "form-elements.checkbox",
        "form-elements.dropdown",
        "form-elements.tile-group",
        "form-elements.date-input",
        "form-elements.files-upload",
        "form-elements.auto-suggest-input",
        "form-elements.hidden-input",
        "form-elements.number-increment",
        "form-elements.dynamic-array"
      ]
    },
    "flow_ids": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::flow-id.flow-id"
    },
    "stepId": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    }
  }
}
