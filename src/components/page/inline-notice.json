{
  "collectionName": "components_page_inline_notice",
  "info": {
    "displayName": "InlineNotice",
    "description": "A notice component with a fixed title label-01-bold. The component works in FormFlow Page, Vorabcheck Page, and Content Page"
  },
  "options": {},
  "attributes": {
    "identifier": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "tagName": {
      "type": "enumeration",
      "enum": ["h1", "h2", "h3", "h4", "h5", "h6", "p", "div"],
      "default": "h1",
      "required": true
    },
    "look": {
      "type": "enumeration",
      "enum": ["warning", "tips"],
      "default": "warning",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "container": {
      "type": "component",
      "repeatable": false,
      "component": "meta.container",
      "required": true
    },
    "outerBackground": {
      "type": "component",
      "repeatable": false,
      "component": "meta.background"
    }
  }
}
