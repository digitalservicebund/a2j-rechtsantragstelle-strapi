{
  "collectionName": "components_page_inline_notice",
  "info": {
    "displayName": "InlineNotice",
    "description": "A notice component with a fixed title label-01-bold. The component works in FormFlow Page, Vorabcheck Page, and Content Page"
  },
  "options": {},
  "attributes": {
    "identifier": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "tagName": {
      "type": "enumeration",
      "required": true,
      "default": "h1",
      "enum": [
        "h1",
        "h2",
        "h3",
        "h4",
        "h5",
        "h6",
        "p",
        "div"
      ]
    },
    "look": {
      "type": "enumeration",
      "required": true,
      "default": "warning",
      "enum": [
        "warning",
        "tips",
        "success",
        "error"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "container": {
      "type": "component",
      "component": "meta.container",
      "repeatable": false,
      "required": true
    },
    "outerBackground": {
      "type": "component",
      "component": "meta.background",
      "repeatable": false
    }
  }
}
