{
  "collectionName": "components_basic_inline_notice",
  "info": {
    "displayName": "Inline Notice",
    "description": "A notice component with a fixed title label-01-bold"
  },
  "options": {},
  "attributes": {
    "identifier": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 70
    },
    "tagName": {
      "type": "enumeration",
      "enum": [
        "h1",
        "h2",
        "h3",
        "h4",
        "h5",
        "h6",
        "p",
        "div"
      ],
      "default": "h1",
      "required": true
    },
    "look": {
      "type": "enumeration",
      "enum": [
        "warning",
        "tips"
      ],
      "default": "warning",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "outerBackground": {
      "type": "component",
      "repeatable": false,
      "component": "meta.background"
    }
  }
}
