{
  "name": "a-2-j-strapi",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "license": "MIT",
  "scripts": {
    "build": "npm run plugin:build && strapi build",
    "develop": "strapi develop",
    "start": "strapi start",
    "strapi": "strapi",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles",
    "transfer": "source .env && strapi transfer",
    "plugin:build": "npm run build --workspace plugins/strapi-plugin-gh-workflow-trigger-v5",
    "plugin:develop": "npm run watch --workspace plugins/strapi-plugin-gh-workflow-trigger-v5"
  },
  "jest": {
    "noStackTrace": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".tmp",
      ".cache",
      "/plugins/"
    ],
    "testTimeout": 70000
  },
  "dependencies": {
    "@strapi/plugin-users-permissions": "5.37.0",
    "@strapi/provider-upload-aws-s3": "5.37.0",
    "@strapi/strapi": "5.37.0",
    "pg": "^8.18.0",
    "pluralize": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.0.0",
    "strapi-plugin-gh-workflow-trigger-v5": "file:plugins/strapi-plugin-gh-workflow-trigger-v5",
    "strapi-v5-plugin-populate-deep": "^4.0.6",
    "styled-components": "^6.3.8"
  },
  "devDependencies": {
    "better-sqlite3": "^12.6.2",
    "jest": "^30.2.0",
    "supertest": "^7.2.2"
  },
  "engines": {
    "node": "^24.14.0",
    "npm": "^11.6.0"
  },
  "strapi": {
    "uuid": "75be5f21-db3a-4a6f-9069-20b0c6b8e10c",
    "telemetryDisabled": true
  },
  "overrides": {
    "tmp": "0.2.5",
    "esbuild": "^0.25.0",
    "glob": "^10.4.5",
    "lodash": "^4.17.23"
  },
  "workspaces": [
    "plugins/strapi-plugin-gh-workflow-trigger-v5"
  ]
}
