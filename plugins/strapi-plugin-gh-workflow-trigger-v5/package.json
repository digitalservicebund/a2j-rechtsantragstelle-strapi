{
  "version": "0.1.0",
  "keywords": [
    "strapi",
    "plugin",
    "github",
    "workflow"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "tsc -p admin/tsconfig.json --noEmit",
    "test:ts:back": "tsc -p server/tsconfig.json --noEmit"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.18",
    "@strapi/icons": "^2.0.0-rc.18",
    "react-intl": "^7.1.9"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.11.3",
    "@strapi/typescript-utils": "^5.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.13",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "gh-workflow-trigger-v5",
    "displayName": "Github Workflow Trigger",
    "description": "Trigger a github workflow from your admin panel"
  },
  "name": "strapi-plugin-gh-workflow-trigger-v5",
  "description": "Trigger a github workflow from your admin panel",
  "license": "MIT",
  "author": "chris <christoph.hohnerlein@digitalservice.bund.de>",
  "prettier": {
    "endOfLine": "lf",
    "tabWidth": 2,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "overrides": {
    "@strapi/pack-up": {
      "vite": "^5.1.8"
    }
  }
}
